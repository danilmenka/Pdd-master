<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetailPaidFine">


    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Номер постановления"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginTop="8dp"
        android:text="Дата постановления"
        app:layout_constraintEnd_toEndOf="@+id/textView11"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Общая сумма штрафа"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        tools:layout_editor_absoluteX="16dp" />

    <ImageView
        android:id="@+id/imageViewOfStaffDetailPai2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewOfStaffDetailPai"
        app:layout_constraintStart_toEndOf="@+id/imageViewOfStaffDetailPai"
        app:layout_constraintTop_toTopOf="@+id/imageViewOfStaffDetailPai"
        tools:srcCompat="@tools:sample/avatars[3]" />

    <ImageView
        android:id="@+id/imageViewOfStaffDetailPai3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewOfStaffDetailPai"
        app:layout_constraintStart_toEndOf="@+id/imageViewOfStaffDetailPai2"
        app:layout_constraintTop_toTopOf="@+id/imageViewOfStaffDetailPai"
        app:layout_constraintVertical_bias="0.0"
        tools:srcCompat="@tools:sample/avatars[3]" />

    <ImageView
        android:id="@+id/imageViewOfStaffDetailPai4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@+id/imageViewOfStaffDetailPai"
        app:layout_constraintStart_toStartOf="@+id/imageViewOfStaffDetailPai"
        app:layout_constraintTop_toBottomOf="@+id/imageViewOfStaffDetailPai"
        tools:srcCompat="@tools:sample/avatars[3]" />

    <ImageView
        android:id="@+id/imageViewOfStaffDetailPai5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewOfStaffDetailPai4"
        app:layout_constraintStart_toEndOf="@+id/imageViewOfStaffDetailPai4"
        app:layout_constraintTop_toTopOf="@+id/imageViewOfStaffDetailPai4"
        tools:srcCompat="@tools:sample/avatars[3]" />

    <ImageView
        android:id="@+id/imageViewOfStaffDetailPai6"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewOfStaffDetailPai4"
        app:layout_constraintStart_toEndOf="@+id/imageViewOfStaffDetailPai5"
        app:layout_constraintTop_toTopOf="@+id/imageViewOfStaffDetailPai4"
        app:layout_constraintVertical_bias="0.0"
        tools:srcCompat="@tools:sample/avatars[3]" />

    <ImageView
        android:id="@+id/imageViewOfStaffDetailPai"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:srcCompat="@tools:sample/avatars[3]" />

    <Button
        android:layout_height="wrap_content"
        android:layout_width="300dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Посмотреть на карте"
        android:id="@+id/butLookMapPaid"
        app:layout_constraintBottom_toTopOf="@+id/butLookDocofPaid"
        android:textColor="@color/colorAccent5"
        android:background="@color/colorAccent3"
        android:layout_marginBottom="16dp"/>

    <TextView
        android:id="@+id/postNumPaid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="111dp"
        android:layout_marginLeft="111dp"
        android:layout_marginTop="8dp"
        android:maxLength="8"
        android:text="1111"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/postNumPaid"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintEnd_toEndOf="@+id/postNumPaid"
        android:text="1111"
        android:layout_marginTop="8dp"
        android:id="@+id/postDatePaid"
        app:layout_constraintTop_toBottomOf="@+id/postNumPaid"
        android:maxLength="8"/>

    <TextView
        android:id="@+id/suma_paid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:maxLength="8"
        android:text="1111"
        app:layout_constraintEnd_toEndOf="@+id/postDatePaid"
        app:layout_constraintStart_toStartOf="@+id/postDatePaid"
        app:layout_constraintTop_toBottomOf="@+id/postDatePaid" />

    <TextView
        android:id="@+id/koap_paid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:maxLength="8"
        android:text="1111"
        app:layout_constraintEnd_toEndOf="@+id/suma_paid"
        app:layout_constraintStart_toStartOf="@+id/suma_paid"
        app:layout_constraintTop_toBottomOf="@+id/suma_paid" />

    <Button
        android:layout_height="wrap_content"
        android:layout_width="300dp"
        app:layout_constraintStart_toStartOf="@+id/butLookMapPaid"
        app:layout_constraintEnd_toEndOf="@+id/butLookMapPaid"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="Посмотреть документ об оплате"
        android:id="@+id/butLookDocofPaid"
        android:textColor="@color/colorAccent5"
        android:background="@color/colorAccent3"
        android:layout_marginBottom="24dp"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Статья КоАП РФ"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Адрес нарушения"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Рег. номер автомобиля"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />


    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Номер водительского удостоверения"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />


    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Фото штрафа"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />


    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/koap_paid"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintEnd_toEndOf="@+id/koap_paid"
        android:text="TextView"
        android:layout_marginTop="8dp"
        android:id="@+id/address_paid"
        app:layout_constraintTop_toBottomOf="@+id/koap_paid"
        android:maxLength="8"/>


    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/address_paid"
        app:layout_constraintEnd_toEndOf="@+id/address_paid"
        android:text="TextView" android:layout_marginTop="8dp"
        android:id="@+id/rernum_paid"
        app:layout_constraintTop_toBottomOf="@+id/address_paid"
        android:maxLength="8"/>


    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/rernum_paid"
        app:layout_constraintEnd_toEndOf="@+id/rernum_paid"
        android:text="TextView"
        android:layout_marginTop="8dp"
        android:id="@+id/vod_paid"
        app:layout_constraintTop_toBottomOf="@+id/rernum_paid"
        android:maxLength="8"/>


</androidx.constraintlayout.widget.ConstraintLayout>